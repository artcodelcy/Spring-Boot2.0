package test;
\
import java.util.concurrent.locks.ReentrantLock;

public class Test15 extends Thread{

	ReentrantLock lock = new ReentrantLock();
	public void get(){
		
		lock.lock();
		System.out.println(Thread.currentThread().getId());
		set();
		lock.unlock();
		
	}
	public void set(){
		lock.lock();
		System.out.println(Thread.currentThread().getId());
		lock.unlock();	
		
	}
	
	public void run(){
       get();
	}
	public static void main(String[] args){
		Test15 ss =  new Test15();
		new Thread(ss).start();
		new Thread(ss).start();
		new Thread(ss).start();
		new Thread(ss).start();
	}
}
