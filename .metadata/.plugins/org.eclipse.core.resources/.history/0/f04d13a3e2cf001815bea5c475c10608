package com.test;

public class Test010 {

	
	public static void main(String[] args){
		Thread  thread = new Thread(
				new Runnable() {
					
					@Override
					public void run() {
						while(true){
						try {
							Thread.sleep(100);
							
						} catch (Exception e) {
							// TODO: handle exception
						}
						System.out.println("加油子线程");
						
					}
					}
				}
				);
	}	
	
}
