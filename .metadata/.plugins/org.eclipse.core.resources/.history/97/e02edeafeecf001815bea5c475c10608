package com.test;

public class Test0111 {
	
		
		   public static void main(String[] args){
			   PriorityThread priorityThread = new PriorityThread();
			   Thread t1 = new Thread(priorityThread);
			   Thread t2 = new Thread(priorityThread);
			   t1.start();
			   t1.setPriority(10);
			   t2.start();
		   } 

			
}
